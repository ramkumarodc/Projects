78. Subsets

void subset(vector<vector<int>>& op, vector<int>& tmp, vector<int>& nums, int idx)
{
    op.push_back(tmp);
    for(int i=idx; i<nums.size(); i++)
    {
        tmp.push_back(nums[i]);
        subset(op, tmp, nums, i+1);
        tmp.pop_back();
    }
}

vector<vector<int>> subsets(vector<int>& nums) 
{
    vector<vector<int>> op;
    vector<int> tmp;
    subset(op, tmp, nums, 0)    ;
    return op;
}

Input
nums = [1,2,3]
Output
[[],[1],[1,2],[1,2,3],[1,3],[2],[2,3],[3]]
Expected
[[],[1],[2],[1,2],[3],[1,3],[2,3],[1,2,3]]

Input
nums =[0]
Output
[[],[0]]
Expected
[[],[0]]