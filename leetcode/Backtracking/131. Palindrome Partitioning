131. Palindrome Partitioning
bool check(string str, int i , int j)
{
    while(i <= j)
    {
        if(str[i] != str[j])
            return false;
        i++;
        j--;
    }
    return true;
}

oid dfs(string &s, vector<vector<string>> &op, vector<string>& tmp, int pos)

   if(pos >=s.size())
   {
       op.push_back(tmp);
   }

   for(int i=pos; i<s.size(); i++)
   {
       int len = i-pos+1;
       string str = s.substr(pos,i-pos+1);
       if(check(str, 0, len-1))
       {               
           tmp.push_back(str);
           dfs(s,op, tmp, i+1);
           tmp.pop_back();
       }
   }



vector<vector<string>> partition(string s) 
{
    vector<vector<string>> op;
    vector<string> tmp;

    dfs(s, op, tmp, 0);
    return op;    
}


Example 1:

Input: s = "aab"
Output: [["a","a","b"],["aa","b"]]
Example 2:

Input: s = "a"
Output: [["a"]]