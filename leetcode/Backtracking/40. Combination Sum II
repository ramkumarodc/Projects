40. Combination Sum II
void combinationSum(vector<int>& nums, vector<vector<int>>& op, vector<int>& tmp, int target, int idx , int sum)
{
    if(sum > target)
    {
        return;
    }
    if(sum == target)
    {
        op.push_back(tmp);
    }
    else
    {
        for(int i=idx; i<nums.size(); i++)
        {
            if(i>idx && nums[i] == nums[i-1]) continue;
            if(nums[i] > target) break;
            tmp.push_back(nums[i]);
            combinationSum(nums, op, tmp, target, i+1, sum+nums[i]);
            tmp.pop_back();
        }
    }
}
vector<vector<int>> combinationSum2(vector<int>& candidates, int target) 
{
    sort(candidates.begin(), candidates.end());
    vector<vector<int>> op;
    vector<int> tmp;
    combinationSum(candidates, op, tmp, target, 0, 0);
    return op;
}


Example 1:

Input: candidates = [10,1,2,7,6,1,5], target = 8
Output: 
[
[1,1,6],
[1,2,5],
[1,7],
[2,6]
]
Example 2:

Input: candidates = [2,5,2,1,2], target = 5
Output: 
[
[1,2,2],
[5]
]