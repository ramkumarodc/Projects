102. Binary Tree Level Order Traversal
vector<vector<int>> levelOrder(TreeNode* root) 
{
    vector<vector<int>> ans;
    if(!root)
        return ans;

    queue<TreeNode*> q;
    q.push(root);
    
    while(!q.empty())
    {
        int s= q.size();
        vector<int> tmp;
        for(int i=0; i<s; i++)
        {
            TreeNode* node  = q.front();
            tmp.push_back(node->val);
            q.pop();
            if(node->left)
                q.push(node->left);
            if(node->right)
                q.push(node->right);
        }
        ans.push_back(tmp);
    }
    return ans;
}